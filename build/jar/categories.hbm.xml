<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    
 <class name="model.beans.Categories" table="categories">
     
  <id name="categorieId" type="long" column="categorie_id" >
   <generator class="native"/>
  </id>
 
 <property name="categorieActive">
   <column name="categorie_active" />
  </property>
  
  <property name="categorieName">
   <column name="categorie_name" />
  </property>
  
  <property name="categorieOrder">
   <column name="categorie_order"/>
  </property>
  
 </class>
 
</hibernate-mapping>